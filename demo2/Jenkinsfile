node{
    def app
    checkout scm
    environment {
        DOCKERHUB_CREDENTIALS=credentials('dockerhub')
    }
    sh 'echo "ayudaaaa"'
    stage('Contruir imagen docker'){
            steps{
                dir("/var/lib/jenkins/workspace/Backend-Ev2/demo2"){
                     sh 'docker build . -t back'    
                 }
            }             
        }

       stage("run") {
                app.inside {
                    sh "ayudaaaa"

                }
          }
    

}