node{
    checkout scm
    environment {
        DOCKERHUB_CREDENTIALS=credentials('dockerhub')
    }
    sh 'echo "ayudaaaa"'
    stage('Docker Build'){
                dir("/var/lib/jenkins/workspace/Tingeso/backend"){
                    sh 'docker build --build-arg JAR_FILE=build/libs/*.jar -t back .'
                }        
        }

    

}